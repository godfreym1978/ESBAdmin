<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from HelloTestService WSDL service -->

  <service name="HelloTest" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://MQAdmin.ibm.com"/>
      <parameter name="wsdlServiceElement" value="HelloTestService"/>
      <parameter name="schemaQualified" value="http://MQAdmin.ibm.com"/>
      <parameter name="wsdlServicePort" value="HelloTest"/>
      <parameter name="className" value="com.ibm.MQAdmin.HelloTestSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="HelloTest"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="returnHello" qname="operNS:returnHello" xmlns:operNS="http://MQAdmin.ibm.com" returnQName="retNS:returnHelloReturn" xmlns:retNS="http://MQAdmin.ibm.com" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <parameter name="allowedMethods" value="returnHello"/>

  </service>
</deployment>
